<section id="sty" ondblclick="blind()">
  <div class="container mt-5">
    {{!-- onclick="location.href='/blindClick/{{this._id}}'" --}}
    <div class="row">
      {{#each products}}
      <div class="col-md-3 p-3">
        <div class="card" style="width: 15rem;">
          <img class="card-img-top" src="./pdtImages/{{this._id}}.jpeg" alt="Card image cap">
          <div class="card-body">
            <h5 class="font-weight-bold" class="card-title">{{this.Name}}</h5>
            <td></td>

            <p class="text-success font-weight-bold font-italic" class="card-text">Price:{{this.Price}}</p>
            <br>
            <p class="font-weight-light" class="card-text">{{this.Dis}}</p>
            <a href="/addToCart/{{this._id}}" class="btn btn-primary">Add to cart</a>
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
  </div>

</section>

